{
  "name": "@enfyra/sdk-next",
  "version": "0.1.6",
  "description": "Next.js SDK for Enfyra CMS",
  "repository": {
    "type": "git",
    "url": "https://github.com/enfyra/sdk-next"
  },
  "homepage": "https://github.com/enfyra/sdk-next#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.cjs"
    },
    "./server/utils/refreshToken": {
      "types": "./dist/server/utils/refreshToken.d.ts",
      "import": "./dist/server/utils/refreshToken.mjs",
      "require": "./dist/server/utils/refreshToken.cjs"
    },
    "./server/utils/proxy": {
      "types": "./dist/server/utils/proxy.d.ts",
      "import": "./dist/server/utils/proxy.mjs",
      "require": "./dist/server/utils/proxy.cjs"
    },
    "./constants/config": {
      "types": "./dist/constants/config.d.ts",
      "import": "./dist/constants/config.mjs",
      "require": "./dist/constants/config.cjs"
    },
    "./constants/auth": {
      "types": "./dist/constants/auth.d.ts",
      "import": "./dist/constants/auth.mjs",
      "require": "./dist/constants/auth.cjs"
    },
    "./proxy": {
      "types": "./dist/proxy.d.ts",
      "import": "./dist/proxy.mjs",
      "require": "./dist/proxy.cjs"
    },
    "./routes/login": {
      "types": "./dist/routes/login.d.ts",
      "import": "./dist/routes/login.mjs",
      "require": "./dist/routes/login.cjs"
    },
    "./routes/logout": {
      "types": "./dist/routes/logout.d.ts",
      "import": "./dist/routes/logout.mjs",
      "require": "./dist/routes/logout.cjs"
    }
  },
  "files": [
    "dist/",
    "templates/",
    "scripts/"
  ],
  "keywords": [
    "nextjs",
    "next",
    "enfyra",
    "cms",
    "sdk"
  ],
  "author": "Enfyra Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "yarn build",
    "postinstall": "node scripts/copy-templates.js"
  },
  "peerDependencies": {
    "next": "^16.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "jwt-decode": "^4.0.0",
    "ofetch": "^1.3.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "next": "^16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

